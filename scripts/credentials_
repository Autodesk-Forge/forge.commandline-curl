#!/bin/bash
#
# Autodesk viewer credentials
#
# Anything following a hash character '#' is ignored.
# This enables you to add comments to the file.

# Specify on separate lines:
#   Consumer Key: [32 characters]
#   Consumer Secret: [16 characters]
ConsumerKey="your_consumer_key"
ConsumerSecret="your_secret_key"
APIHost="https://developer.api.autodesk.com/"

# If you want to post your html page on a different localhost than the default one,
# specify which one below
LocalhostFolder=/Library/WebServer/Documents # http://localhost/
#LocalhostFolder=~/Sites # http://localhost/~${USER}/

# Do not modify the following lines
access_token=""
[[ -f $ROOT/data/access_token ]] && access_token=`cat $ROOT/data/access_token`

bucket=""
[[ -f $ROOT/data/bucket ]] && bucket=`cat $ROOT/data/bucket`
